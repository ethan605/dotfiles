{
  "title": "RCmd to navigate apps",
  "rules": [
    {
      "description": "RCmd+RShift to navigate apps",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["right_shift"]
            }
          },
          "to": [{ "shell_command": "open -b org.alacritty" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [
            { "key_code": "spacebar", "modifiers": ["command", "control"] }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "c",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [{ "shell_command": "open -b com.google.Chrome" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "f",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [{ "shell_command": "open -b org.nixos.firefox" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "l",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [{ "shell_command": "open -b com.linear" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "n",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [{ "shell_command": "open -b notion.id" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "s",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [{ "shell_command": "open -b com.tinyspeck.slackmacgap" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "t",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [{ "shell_command": "open -b ru.keepcoder.Telegram" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "w",
            "modifiers": { "mandatory": ["right_command", "right_shift"] }
          },
          "to": [{ "shell_command": "open -b net.whatsapp.WhatsApp" }],
          "type": "basic"
        }
      ]
    }
  ]
}
