    {
      "description": "Voyager - RCmd+Space to navigate terminal",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": { "mandatory": ["right_command"] }
          },
          "to": [{ "shell_command": "open -a Alacritty.app" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": { "mandatory": ["right_command", "shift"] }
          },
          "to": [{ "shell_command": "open -n -a Alacritty.app" }],
