{
  "title": "ZSA Voyager replication",
  "rules": [
    {
      "description": "Voyager - Esc/LCtrl, Tab/Fn, and LCmd/Enter dual functions",
      "manipulators": [
        {
          "from": { "key_code": "left_control" },
          "parameters": { "basic.to_if_alone_timeout_milliseconds": 200 },
          "to": [{ "key_code": "left_control" }],
          "to_if_alone": [{ "key_code": "escape" }],
          "type": "basic"
        },
        {
          "from": { "key_code": "tab" },
          "parameters": { "basic.to_if_alone_timeout_milliseconds": 200 },
          "to": [{ "key_code": "fn" }],
          "to_if_alone": [{ "key_code": "tab" }],
          "type": "basic"
        },
        {
          "from": { "key_code": "left_command" },
          "parameters": { "basic.to_if_alone_timeout_milliseconds": 200 },
          "to": [{ "key_code": "left_command" }],
          "to_if_alone": [{ "key_code": "return_or_enter" }],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Voyager - RCmd+h/j/k/l as arrow keys",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "left_arrow" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "down_arrow" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "up_arrow" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "right_arrow" }],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Voyager - Fn+number row for media functions",
      "manipulators": [
        {
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [
            {
              "key_code": "q",
              "modifiers": ["command", "control"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "1",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "key_code": "display_brightness_decrement" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "2",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "key_code": "display_brightness_increment" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "3",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "key_code": "vk_none" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "4",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "key_code": "vk_none" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "5",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "key_code": "f12" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "6",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "consumer_key_code": "rewind" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "7",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "consumer_key_code": "play_or_pause" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "8",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "consumer_key_code": "fast_forward" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "9",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "consumer_key_code": "mute" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "0",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "consumer_key_code": "volume_decrement" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "hyphen",
            "modifiers": { "mandatory": ["fn"] }
          },
          "to": [{ "consumer_key_code": "volume_increment" }],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Voyager - Top right cluster",
      "manipulators": [
        {
          "from": { "key_code": "delete_or_backspace" },
          "to": [{ "key_code": "vk_none" }],
          "type": "basic"
        },
        {
          "from": { "key_code": "equal_sign" },
          "to": [{ "key_code": "vk_none" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "hyphen",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["shift"]
            }
          },
          "to": [{ "key_code": "equal_sign" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "hyphen",
            "modifiers": { "mandatory": ["left_control"] }
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": ["command"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "hyphen",
            "modifiers": { "optional": ["left_option"] }
          },
          "to": [{ "key_code": "delete_or_backspace" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["shift"]
            }
          },
          "to": [{ "key_code": "hyphen" }],
          "type": "basic"
        },
        {
          "from": { "key_code": "backslash" },
          "to_if_alone": [{ "key_code": "vk_none" }],
          "type": "basic"
        },
        {
          "from": { "key_code": "close_bracket" },
          "to_if_alone": [{ "key_code": "vk_none" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "open_bracket",
            "modifiers": { "optional": ["shift"] }
          },
          "to": [{ "key_code": "backslash" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "close_bracket" }],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "open_bracket" }],
          "type": "basic"
        },
        {
          "from": { "key_code": "return_or_enter" },
          "to": [{ "key_code": "vk_none" }],
          "type": "basic"
        }
      ]
    },
